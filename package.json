    "pkgTs": "yarn clean && tsc --build tsconfig.json",
    "runJs": "yarn build && node ./app/index.js",
    "runTs": "ts-node -r tsconfig-paths/register ./src/app.ts --files",
    "dev": "nodemon --config ./nodemon.json -w src/**/*.ts -x \"ts-node -r tsconfig-paths/register --files\" ./src/app.ts",
    "dev:debug": "nodemon --config ./nodemon.json --inspect-brk ./src/app.ts",
    "build": "yarn clean && node ./.debug/preBuild.js && cross-env NODE_ENV=prod  yarn pkgTs && node ./.debug/postBuild.js",
    "try": "node ./example/app.js",
    "lint": "eslint . --ext .js"
  },
